{
  "name": "aldeebhasan/emigrate",
  "description": "This package is designed to enable the developer to generate the migration files by just building his model",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "migrate",
    "laravel migrate",
    "model migrate",
    "handle modle",
    "laravel"
  ],
  "authors": [
    {
      "name": "Hasan Deeb",
      "email": "aldeeb.91@gmail.com",
      "homepage": "https://hasan-deeb.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Aldeebhasan\\Emigrate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aldeebhasan\\Emigrate\\Test\\": "tests/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "orchestra/testbench": "8.x-dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aldeebhasan\\Emigrate\\EmigrateProvider"
      ],
      "aliases": {
      }
    }
  },
  "scripts": {
    "fix": "php-cs-fixer fix src",
    "test": "vendor/bin/phpunit tests/Unit/EmigrateTest.php"
  }
}
